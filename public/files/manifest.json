{
  "manifest_version": 3,
  "minimum_chrome_version": "116",
  "name": "Bitcoin blocks tracker",
  "version": "0.0.0",
  "version_name": "0.0.0",
  "default_locale": "en",

  "icons": {
    "16":"icons/16.png",
    "32":"icons/32.png",
    "48":"icons/48.png",
    "128":"icons/128.png"
  },

  "action": {
    "default_popup": "popup.html"
  },

  "background": {
    "service_worker": "js/background.js"
  },

  "permissions": ["storage", "offscreen", "notifications"]
}
